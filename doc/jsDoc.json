{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["./src/"],
    "exclude": ["./dist/", "./doc/", "./node_modules/", "./test/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "name": "KolorPick",
    "footerText": "KolorPick",
    "cleverLinks": false,
    "monospaceLinks": false,
    "logo": {
      "url": "../demo/logo.png",
      "width": "20px",
      "height": "20px",
      "link": "https://github.com/ArthurBeaulieu/KolorPick"
    }
  },
  "opts": {
    "destination": "./doc/",
    "template": "templates/default",
    "encoding": "utf8",
    "readme": "README.md",
    "package": "",
    "access": "all",
    "recurse": true,
    "verbose": true,
    "private": true,
    "pedantic": true
  }
}
